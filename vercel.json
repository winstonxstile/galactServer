{
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.6.0"
    }
  },
  "routes": [
    {
      "src": "/api",
      "dest": "api/image.png"
    },
    {
      "src": "/(css|js|images)/(.*)",
      "dest": "api/$1"
    },
    {
      "src": "/",
      "dest": "/api/index.php"
    },
    {
      "src": "/(.*)",
      "dest": "/api/image.png"
    }
  ]
}
